                                <path d="M156.8,66.5c-15,0-19.6-4-19.6-9.1l0-18.2h9.9v17.2c0,4.2,4.4,5.3,10.2,5.3c4,0,7-0.1,9.2-0.4V39.1h9.9v26.1
                       C172.6,65.9,165.2,66.5,156.8,66.5z"></path>
                                <path fill="#005DA5" d="M282,60.8c0-1.3,0.8-1.8,3.1-1.8h3.9c2.3,0,3.1,0.5,3.1,1.8V64c0,1.3-0.8,1.8-3.1,1.8h-3.9
                            c-2.3,0-3.1-0.5-3.1-1.8V60.8z"></path>
                                <path fill-rule="evenodd" clip-rule="evenodd" d="M108.1,49.4c-6.3,0-12,0.6-15.9,1.3c-2.8,0.5-4.1,1.5-4.1,3.9v6.7
